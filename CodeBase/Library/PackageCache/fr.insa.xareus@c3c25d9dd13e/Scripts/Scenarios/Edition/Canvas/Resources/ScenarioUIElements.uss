{
    -unity-font-definition: resource('Inter-Regular SDF');
    white-space: nowrap;
    color: #FFFFFF;

}

.node-label {
    align-self: center;
    font-size: 15px;
    top: -20px;
    left: 0px;
    -unity-text-align: middle-center;
}

.priority-label {
    position : absolute;
    align-self: center;
    font-size: 15px;
    top: 5px;
    -unity-text-align: middle-center;
    color: #000000;
}

.unfiltered,
.unfiltered * {
    -unity-background-image-tint-color: #555555;
    color: #777777;
}

.bypass > * {
    -unity-background-image-tint-color: #999999;
    color: #999999;
}



.sensor-number-label {
    position : absolute;
    align-self: flex-end;
    font-size: 9px;
    top: 14px;
    right : -1px;
    -unity-text-align: middle-right;
    color: #EAC426;
}

.effector-number-label {
    position : absolute;
    font-size: 9px;
    top: 14px;
    left : -1px;
    -unity-text-align: middle-left;
    color: #EAC426;
}



